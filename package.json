{
  "name": "next-with-bugsnag",
  "version": "0.1.0",
  "main": "dist/index.js",
  "umd:main": "dist/next-with-bugsnag.umd.production.js",
  "module": "dist/next-with-bugsnag.es.production.js",
  "typings": "dist/index.d.ts",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --env=jsdom",
    "release": "np --help"
  },
  "peerDependencies": {
    "next": ">=8",
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "np": {
    "yarn": true,
    "contents": "dist"
  },
  "devDependencies": {
    "@types/jest": "^24.0.13",
    "@types/next": "^8.0.5",
    "@types/react": "^16.8.19",
    "@types/react-dom": "^16.8.4",
    "husky": "^2.4.0",
    "next": "11.1.0",
    "np": "^5.0.3",
    "prettier": "^1.18.0",
    "pretty-quick": "^1.11.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "tsdx": "^0.6.1",
    "tslib": "^1.9.3",
    "typescript": "^3.5.1"
  },
  "dependencies": {
    "@bugsnag/js": "^6.3.0",
    "@bugsnag/plugin-react": "^6.2.0"
  }
}
